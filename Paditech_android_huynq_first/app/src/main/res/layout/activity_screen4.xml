<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.pesteam.paditech_android_huynq_first.Screen4Activity">
    <include
        android:id="@+id/tool_bar_include"
        layout="@layout/tool_bar_top_screen4"/>

    <android.support.v7.widget.RecyclerView
        android:layout_below="@+id/tool_bar_include"
        android:id="@+id/rcv_new_Screen4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <include
        layout="@layout/tool_bar_bottom_screen4"
        android:layout_alignBottom="@+id/rcv_new_Screen4"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />

</RelativeLayout>
